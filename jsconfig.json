{
	"compilerOptions": {
		"paths": {
			"$lib":["src/lib"],
			"$lib/*":["src/lib/*"],
			"$services": [
				"src/lib/services/index.js"
			],
			"$services/*": [
				"src/lib/services/*"
			],
			"$utils": [
				"src/lib/utils"
			],
			"$utils/*": [
				"src/lib/utils/*"
			],
		}
	},
	"extends": "./.svelte-kit/tsconfig.json",
}
